pipeline{
    agent any
    stages{

        stage('git clone statge') { // for display purposes
            steps{
                git url: 'https://github.com/nader1542/test_pipeline.git'
            }
        }
        stage ('clean install stage'){
            steps{
                withMaven(maven : 'M3'){
                    sh 'mvn clean install -Dmaven.test.skip=true'
                }
             }
        }
    }
}
